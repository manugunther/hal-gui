

vardef b : Bool;
vardef q : Int;

q := 0 - 0;

b := not True;

skip;

do (q < 10) ->
    
    if False           -> q  := 100;
    | (q = 0 - q)  -> q := 100;
    | not b            -> b := False;
    | False && b -> q := 100;
    | b                     -> b := True;
    fi
    
    q := q + 1;
    
od

q := 0;
